{
  "name": "redux-hist-react",
  "version": "0.0.7",
  "description": "React Components for Redux around History API.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "./node_modules/.bin/tslint ./src/*.tsx ./test/*.tsx",
    "build": "rm -rf ./lib && ./node_modules/.bin/tsc -p .",
    "test": "rm -rf ./test-dist && ./node_modules/.bin/tsc --outDir ./test-dist ./test/*.tsx && ./node_modules/.bin/mocha ./test-dist/*.js && rm -rf ./test-dist",
    "release": "npm run lint && npm run build && npm test && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minodisk/redux-hist.git"
  },
  "keywords": [
    "component",
    "container",
    "history",
    "location",
    "react",
    "redux"
  ],
  "author": "Daisuke Mino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minodisk/redux-hist/issues"
  },
  "homepage": "https://github.com/minodisk/redux-hist/tree/master/packages/redux-hist-react#readme",
  "dependencies": {
    "react": "^15.6.1",
    "react-redux": "^5.0.5",
    "redux-hist": "0.0.7"
  },
  "devDependencies": {
    "@types/history": "^4.6.0",
    "@types/mocha": "^2.2.41",
    "@types/react": "^15.0.38",
    "@types/react-redux": "^4.4.46",
    "@types/redux-actions": "^1.2.6",
    "mocha": "^3.4.2",
    "typescript": "^2.4.1"
  }
}
