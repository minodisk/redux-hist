{
  "name": "redux-hist",
  "version": "0.0.8",
  "description": "Middlewares / Actions / Reducers / Router for Redux around History API.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "lint": "./node_modules/.bin/tslint ./src/*.ts ./test/*.ts",
    "build": "./node_modules/.bin/tsc -p .",
    "pretest": "./node_modules/.bin/tsc --outDir ./test-dist ./test/*.ts",
    "test": "./node_modules/.bin/nyc ./node_modules/.bin/mocha ./test-dist/*.js",
    "posttest": "nyc report --reporter=json",
    "codecov": "[ \"$CI\" = \"true\" ] && codecov -f coverage/*.json",
    "prepublishOnly": "npm run lint && npm run build && npm test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minodisk/redux-hist.git"
  },
  "keywords": [
    "history",
    "location",
    "redux",
    "router",
    "routing",
    "route"
  ],
  "author": "Daisuke Mino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minodisk/redux-hist/issues"
  },
  "homepage": "https://github.com/minodisk/redux-hist/tree/master/packages/redux-hist#readme",
  "dependencies": {
    "path-to-regexp": "^1.7.0",
    "redux-actions": "^2.2.1"
  },
  "devDependencies": {
    "@types/history": "^4.6.0",
    "@types/mocha": "^2.2.41",
    "@types/power-assert": "^1.4.29",
    "@types/redux": "^3.6.0",
    "@types/redux-actions": "^1.2.6",
    "codecov": "^2.2.0",
    "history": "^4.6.3",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "power-assert": "^1.4.4",
    "tslint": "^5.5.0",
    "typescript": "^2.4.1"
  }
}
