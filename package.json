{
  "name": "redux-hist",
  "version": "0.0.12",
  "description":
    "Middlewares / Actions / Reducers / Router for Redux around History API.",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": ["lib"],
  "directories": {
    "test": "test"
  },
  "scripts": {
    "format":
      "./node_modules/.bin/prettier --write ./src/*.ts ./text/*.ts ./*.json && ./node_modules/.bin/tslint --fix ./src/*.ts ./test/*.ts",
    "lint": "./node_modules/.bin/tslint ./src/*.ts ./test/*.ts",
    "build": "./node_modules/.bin/tsc -p .",
    "pretest": "./node_modules/.bin/tsc --outDir ./test-dist ./test/*.ts",
    "test":
      "./node_modules/.bin/nyc ./node_modules/.bin/mocha ./test-dist/*.js",
    "posttest":
      "nyc report --reporter=json && if [ \"$CI\" = \"true\" ]; then codecov -f coverage/*.json; fi"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minodisk/redux-hist.git"
  },
  "keywords": ["history", "location", "redux", "route", "router", "routing"],
  "author": "Daisuke Mino",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/minodisk/redux-hist/issues"
  },
  "homepage": "https://github.com/minodisk/redux-hist#readme",
  "dependencies": {
    "path-to-regexp": "^1.7.0",
    "redux-actions": "^2.2.1"
  },
  "devDependencies": {
    "@types/history": "^4.6.0",
    "@types/mocha": "^2.2.41",
    "@types/power-assert": "^1.4.29",
    "@types/redux-actions": "^1.2.6",
    "codecov": "^2.2.0",
    "history": "^4.6.3",
    "mocha": "^3.4.2",
    "nyc": "^11.0.3",
    "power-assert": "^1.4.4",
    "prettier": "^1.7.4",
    "redux": "^3.7.2",
    "tslint": "^5.8.0",
    "tslint-config-prettier": "^1.6.0",
    "typescript": "^2.4.1"
  }
}
